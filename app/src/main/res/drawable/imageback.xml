<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape
            android:shape="rectangle" >
            <gradient
                android:angle="90"
                android:startColor="@color/accent_material_light"
                android:centerColor="#eeeee1"
                android:endColor="#55000066"
                android:type="linear"
                />
            <stroke android:width="10dp"
                android:color="@color/highlighted_text_material_light" />
            <corners
                android:radius="5dp"/>
        </shape>
    </item>
</layer-list>